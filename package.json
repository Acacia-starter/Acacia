{
  "name": "akaru-starter",
  "version": "1.0.0",
  "license": "ISC",
  "description": "Configurable static site generator",
  "keywords": [
    "landing",
    "webpack",
    "es6",
    "babel",
    "stylus",
    "sass"
  ],
  "bugs": {
    "url": "https://github.com/quentinneyraud/akaru-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quentinneyraud/akaru-starter.git"
  },
  "homepage": "https://quentinneyraud.github.io/akaru-starter",
  "author": {
    "name": "Quentin Neyraud",
    "email": "quentin.neyraud@gmail.com",
    "url": "https://github.com/quentinneyraud"
  },
  "contributors": [
    {
      "name": "Quentin Neyraud",
      "email": "quentin.neyraud@gmail.com",
      "url": "https://github.com/quentinneyraud"
    }
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch config --watch akaru.config.js --quiet --exec 'node bin/dev.js'",
    "debug:routes": "node bin/debug-routes.js",
    "generate:analyze": "npm run lint && cross-env NODE_ENV=production ANALYZE=true node bin/generate.js",
    "generate": "npm run lint && cross-env NODE_ENV=production node bin/generate.js",
    "generate:zip": "npm run lint && cross-env NODE_ENV=production ZIP=true node bin/generate.js",
    "build": "npm run generate",
    "lint": "eslint assets/js components pages",
    "lint:fix": "eslint --fix assets/js components pages",
    "serve": "serve ./generate",
    "postinstall": "link-module-alias"
  },
  "_moduleAliases": {
    "nunjucks": "./node_modules/@akaru/nunjucks"
  },
  "dependencies": {
    "@akaru/nunjucks": "^3.2.3",
    "@qneyraud/q-stylus": "^1.5.4",
    "@qneyraud/q-utils": "^1.5.0",
    "intersection-observer": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.0.0",
    "@qneyraud/eslint-config": "1.0.3",
    "autoprefixer": "^9.1.3",
    "babel-eslint": "8.2.1",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "core-js": "3",
    "critters-webpack-plugin": "^2.3.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "defu": "^0.0.3",
    "dotenv-webpack": "^1.7.0",
    "eslint": ">=5",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "extra-watch-webpack-plugin": "^1.0.3",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "link-module-alias": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.0",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.2",
    "serve": "^10.1.2",
    "simple-nunjucks-loader": "^1.0.0-alpha.9",
    "style-loader": "^0.22.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^2.2.2",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "zip-webpack-plugin": "^3.0.0"
  }
}
